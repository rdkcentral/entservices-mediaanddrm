precondition = ["Platform"]
callsign = "org.rdk.ScreenCapture"
autostart = "false"
startuporder = "@PLUGIN_SCREENCAPTURE_STARTUPORDER@"

configuration = JSON()
rootobject = JSON()

rootobject.add("mode", "@PLUGIN_SCREENCAPTURE_MODE@")
